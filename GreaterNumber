
public class GreatNumber {
	int num1 = 10;
	int num2 = 20;

	public static void main(String[] args) {
		GreatNumber gn=new GreatNumber();
		gn.checkGreater();
	}
	void checkGreater() {
		if(num1>num2)
		{
			System.out.println("Num1 is greater");
			}
		else {
			System.out.println("Num2 is greater");
		}
	}
	}
